	<script src="js/jquery-3.4.1.slim.min.js"></script>
	<script src="js/jquery-2.2.4.min.js"></script>
	<script src="js/popper.min.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script>
		(function ($) {
			// $('.navigation a.dropdown-toggle').on('click', function(e) { 
			// 	$('.navigation .dropdown-menu .show').removeClass("show");
			// });
			$('.navigation .dropdown-menu a.dropdown-toggle').on('click', function (e) {
				$('.navigation .dropdown-menu a.dropdown-toggle').removeClass('active');
				$(this).addClass('active');
				if (!$(this).next().hasClass('show')) {
					$(this).parents('.navigation .dropdown-menu').first().find('.show').removeClass("show");
				}
				var $subMenu = $(this).next(".navigation .dropdown-menu");
				$subMenu.toggleClass('show');

				$(this).parents('.navigation li.nav-item.dropdown.show').on('hidden.bs.dropdown', function (e) {
					$('.navigation .dropdown-menu.show').removeClass("show");
				});

				return false;
			});


		})(jQuery)

	</script>

	<!-- Swiper JS -->
	<script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>

	<!-- Initialize Swiper -->
	<script>
		var swiper = new Swiper(".mySwiper", {
			slidesPerView: 5, loop: true,
			spaceBetween: 40,
			navigation: {
				nextEl: ".swiper-button-next",
				prevEl: ".swiper-button-prev",
			},autoplay: {
    delay: 1000,
  },
			 // Responsive breakpoints
			  breakpoints: {
				// when window width is >= 320px
				320: {
				  slidesPerView: 2,
				  spaceBetween: 20
				},
				// when window width is >= 480px
				480: {
				  slidesPerView: 3,
				  spaceBetween: 30
				},
				// when window width is >= 640px
				640: {
				  slidesPerView: 5,
				  spaceBetween: 40
				}},
		});
	</script>

