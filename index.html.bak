	$('.navigation .dropdown-menu a.dropdown-toggle').on('click', function(e) { 
		$('.navigation .dropdown-menu a.dropdown-toggle').removeClass('active');
		$(this).addClass('active');
	  if (!$(this).next().hasClass('show')) {
		$(this).parents('.navigation .dropdown-menu').first().find('.show').removeClass("show");
	  }
	  var $subMenu = $(this).next(".navigation .dropdown-menu");
	  $subMenu.toggleClass('show');

	  $(this).parents('.navigation li.nav-item.dropdown.show').on('hidden.bs.dropdown', function(e) {
		$('.navigation .dropdown-menu.show').removeClass("show");
	  });

	  return false;
	});
})(jQuery)
